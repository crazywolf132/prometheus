{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "out": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "prepare": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
